<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>Admin - Dashboard</title>
    <link rel="shortcut icon" href="assets/img/favicon.png">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,500;0,600;0,700;1,400&amp;display=swap">
    <link rel="stylesheet" href="assets/plugins/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="assets/plugins/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="assets/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>

    <style>
        .logo1 {
            float: left;
            justify-items: left;
            margin-top: 14px;

        }

        .logo2 {
            margin-left: 15px;
        }

        .btn {
            margin-top: 10px;
        }

        .flex {
            display: flex;
            justify-content: center;
        }

        .hr {
            height: 2px;
            background-color: rgb(9, 7, 7);
            border: none;
        }

        hr {
            height: 1px;
            background-color: rgb(9, 7, 7);
            border: none;
        }

        .btn2 {
            font-size: 22px;
            margin-top: 8px;
            margin-left: 15px;
            width: 200px;
            height: 50px;
        }

        .btn1 {
            font-size: 20px;
            margin-top: 2px;
            margin-left: 15px;
            width: 160px;
            height: 42px;
        }
    </style>

</head>

<body>
    <!--Common Header File -->
    <div id="headnavbar">
    </div>
    <script>
        $(document).ready(function () {
            $('#headnavbar').load("/common/header.hbs");
        });
    </script>

    <div class="main-wrapper">
        <div class="sidebar" id="sidebar">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li>
                            <div class="flex">
                                {{#if imge}}
                                <img src="backend/images/{{imge}}" class="img-fluid" alt="">
                                {{else}}
                                <img src="backend/images/7.jpg" class="img-fluid" alt="">
                                {{/if}}
                            </div>
                        </li>
                        <li class="menu-title">
                            <span>Main Menu</span>
                        </li>
                        <li class="submenu">
                            <a href="student"><i class="bi bi-person-circle"></i> <span> Student Dashboard</span><span
                                    class="menu-arrow"></span></a>
                            <ul>
                                <li><a href="student">Back To Student Page</a></li>
                            </ul>
                        </li>
                        <li class="">
                            <a href="student-profile"><i class="fas fa-user-graduate"></i> <span> My Profile </span>
                                <span class="menu-arrow"></span></a>
                        </li>

                        <li class="menu-title">
                            <span>Chat Application</span>
                        </li>
                        <li>
                            <a href="student-chatapp"><i class="fas fa-holly-berry"></i> <span>Chat App</span></a>
                        </li>
                        <li>
                            <a href="student-feedback"><i class="fas fa-clipboard-list"></i> <span>Feedback</span><span
                                    class="menu-arrow"></span></a>
                        </li>
                        <li class="menu-title">
                            <span>Notes</span>
                        </li>
                        <li class="submenu active">
                            <a href="student-viewnotes"><i class="fas fa-file"></i> <span>View Notes</span>
                                <span class="menu-arrow"></span></a>
                        </li>
                        <li class="menu-title">
                            <span>Pages</span>
                        </li>

                        <li>
                            <a href="blank-page.html"><i class="fas fa-file"></i> <span>Blank Pages</span></a>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="page-wrapper">
        <div class="content container-fluid">
            <div class="card" style="float:left;margin-left: 0px;width:100%">
                <div class="card-body">
                    <h2>View Notes</h2>
                    <hr class="hr">
                    <div class="card">
                        <div class="card-body">
                            <div style="display: flex;">
                                <div class="col-md-6 mb-3">
                                    <label class="form-label">Select Your Subject Teacher</label>
                                    <select class="form-control form-control-lg" name="email" id="email">
                                        {{#each emails}}
                                        <option>{{this}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                                <div class="col-md-6 mb-3">
                                    <label class="form-label">Choose The Sem</label>
                                    <select class="form-control form-control-lg" name="sem" id="sem">
                                        <option>Sem1</option>
                                        <option>Sem2</option>
                                        <option>Sem3</option>
                                        <option>Sem4</option>
                                        <option>Sem5</option>
                                        <option>Sem6</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-12 mb-3">
                                <button type="button" class="btn btn-primary btn2" id="clickhere">Click
                                    Here</button>
                            </div>
                        </div>
                    </div>
                    <!-- Your HTML -->
                    <div class="card">
                        <div class="card-body" id="cardBody">

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <script>
        function openPdf(email, semester, filename) {
            window.open(`/View/${email}/${semester}/${filename}`, '_blank');
        }
    </script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/notes.js"></script>
    <script src="assets/js/jquery-3.6.0.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="assets/plugins/apexchart/apexcharts.min.js"></script>
    <script src="assets/plugins/apexchart/chart-data.js"></script>
    <script src="assets/js/script.js"></script>

</body>

</html>